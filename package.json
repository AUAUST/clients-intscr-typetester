{
  "name": "auaust-projects-typetester",
  "license": "UNLICENSED",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public vendor/getkirby/cms/router.php\"",
    "dev": "shx rm -rf public/dist && concurrently \"npm:kirby\" \"vite\"",
    "build": "vite build",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,vue}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.11",
    "unplugin-vue-components": "^0.22.12",
    "@prettier/plugin-php": "^0.19.2",
    "concurrently": "^7.6.0",
    "shx": "^0.3.4",
    "prettier": "2.8.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.31.0"
  }
}
